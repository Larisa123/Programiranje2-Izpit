import java.io.*;
import java.util.*;

public class branjeDatotek {

	public static void main(String[] args) throws IOException {
		int stevilo = preberiDatoteko("/vhod4.txt");
		System.out.print(stevilo);

	}
	
	public static int preberiDatoteko(String imeDatoteke) throws IOException {
		FileReader fileReader = new FileReader(imeDatoteke);
		BufferedReader br = new BufferedReader(fileReader);
		
		int stevec = 0;
		while (br.readLine() != null) {
			String line = br.readLine();
			System.out.println(line);
		}
		
		fileReader.close();
		return stevec;
	}

}
